{
  "name": "@pie-lib/pie-lib",
  "description": "the pie-libs mono repo",
  "private": true,
  "license": "MIT",
  "config": {
    "scope": "none"
  },
  "engines": {
    "node": "^11.2.0"
  },
  "scripts": {
    "static": "./scripts/static",
    "test": "./scripts/test",
    "dev": "./scripts/dev",
    "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx packages/**/src",
    "lint-two": "lerna exec -- \\$LERNA_ROOT_PATH/node_modules/.bin/eslint  --ignore-path \\$LERNA_ROOT_PATH/.eslintignore --ext .js --ext .jsx src",
    "clean": "lerna exec -- rm -fr lib node_modules",
    "release": "npm run clean && npm run build && npm test && lerna publish --conventional-commits --yes",
    "release:next": "npm run build && npm run test && lerna publish --canary --yes --conventional-commits --preid next --dist-tag next",
    "now:next": "npm run build && scripts/wip pie-lib-next",
    "build": "npm run clean && npm run lint-two && npm run babelize",
    "build:light": "npm run babelize",
    "babelize": "lerna exec -- \\$LERNA_ROOT_PATH/scripts/babelize \\$LERNA_PACKAGE_NAME",
    "postinstall": "lerna bootstrap"
  },
  "devDependencies": {
    "@babel/runtime": "7.2.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@pie-framework/build-helper": "^1.3.3",
    "@pie-lib/test-utils": "^0.0.3",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^24.0.5",
    "@types/react": "^16.7.20",
    "@types/fs-extra": "^5.0.5",
    "@zeit/next-css": "^1.0.2-canary.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "jest": "24.1.0",
    "lerna": "^3.11.0",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "next": "^8.0.1",
    "now": "^13.1.3",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "yarn": "^1.13.0"
  },
  "dependencies": {
    "@material-ui/codemod": "^1.1.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
